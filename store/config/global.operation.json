{
  "cacheConfig": {
    "enabled": false,
    "maxAge": 120,
    "public": false,
    "staleWhileRevalidate": 30
  },
  "liveQueryConfig": {
    "enabled": false,
    "pollingIntervalSeconds": 10
  },
  "graphqlTransformEnabled": true,
  "authenticationConfigs": {
    "2": {
